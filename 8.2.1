static void zad_8_2_1()
        {
            Console.WriteLine("podaj 5 liczb");
            int n = 5;
            int[] tab = new int[n];
            for(int i = 0; i < n; i++)
            {
                Console.WriteLine("podaj liczbe: ");
                int liczba = int.Parse(Console.ReadLine());
                tab[i] = liczba;
            }
            int max = tab[0];
            foreach (int row in tab)
            {
                if (row > max)
                {
                    max = row;
                }
            }
            Console.WriteLine(max);
        }
